#ifndef FFT_H_
#define FFT_H_
#include "main.h"
#include "usbd_cdc_if.h"
#include "arm_math.h"

#define FFT_LENGTH 4096

void fft_start();

typedef struct
{
	uint32_t chirp   = 16;
	uint32_t ADC_N   = 600;
	uint32_t numRX   = 1;
	uint64_t fs      = 2.667e6;
	uint64_t N       = FFT_LENGTH;
	uint64_t N1      = 2048;
	double   T       = 235e-6;
	uint64_t B       = 3.13e9;
	uint64_t f_begin = 78e9;
	uint64_t c       = 2.99792458e8;
	double   gap     = fs;
} radar_param_t;

//x_frequent = n * param.fs / param.N;
//x_range = x_frequent * param.T * param.c / (2 * param.B);

#endif
