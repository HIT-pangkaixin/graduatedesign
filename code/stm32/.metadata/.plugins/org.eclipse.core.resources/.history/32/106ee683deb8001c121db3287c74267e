#ifndef FFT_H_
#define FFT_H_
#include "main.h"
#include "usbd_cdc_if.h"
#include "arm_math.h"

#define FFT_LENGTH 4096

#define chirp    (16)
#define ADC_N    (600)
#define numRX    (1)
#define fs       (2.667e6)
#define N        (FFT_LENGTH)
#define N1       (2048)
#define T        (235e-6)
#define B        (3.13e9)
#define f_begin  (78e9)
#define c        (2.99792458e8)
#define frequent (fs / N)
#define gap      (frequent * T * c / (2 * B))

void fft_start();
uint32_t find_fft_max();
double caculate_range();



#endif
